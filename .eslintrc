{
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "mocha": true
    },

    "globals": {
        //"TEST": true
    },

    "rules": {
        "quote": [ 1, "single" ],
        "comma-style": [ 1, "last" ], //カンマ区切りの場合末尾にカンマつける
        "consistent-this": [ 1, "self" ], //thisはselfで統一
        "indent": [ 1, 4 ],
        "curly": 1, //中括弧強制
        "jsx-quotes": [ 1, "prefer-double" ],
        "key-spacing": [ 1, { "beforeColon": true, "afterColon": true } ],
        "no-spaced-func": 1,
        "no-underscore-dangle": 1,
        "no-multi-spaces": 0
    },

    "ecmaFeatures": {
        "modules": true,
        "jsx": true
    },

    "plugins": [
        "react"
    ]
}

